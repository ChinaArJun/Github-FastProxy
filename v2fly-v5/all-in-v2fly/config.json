{
  "log": {
    "access": {
      "type": "File",
      "path": "/tmp/v2ray/access.log",
      "level": "Info"
    },
    "error": {
      "type": "File",
      "path": "/tmp/v2ray/_error.log",
      "level": "Info"
    }
  },
  "inbounds": [
    {
      "protocol": "shadowsocks",
      "port": 11000,
      "settings": {
        "method": "chacha20-ietf-poly1305",
        "password": "your password",
        "networks": "tcp,udp"
      },
      "streamSettings": {
        "transport": "ws",
        "security": "none",
        "transportSettings": {
          "path": "/vmess"
        }

      }
    },
    {
      "port": 12000,
      "protocol": "vmess",
      "settings": {
        "users": [ "your user id" ]
      },
      "streamSettings": {
        "transport": "ws",
        "security": "none",
        "transportSettings": {
          "path": "/vmess"
        }

      }
    },
    {
      "port": 8080,
      "protocol": "trojan",
      "settings": {
        "users": [ "your password"]
      },
      "streamSettings": {
        "transport": "ws",
        "security": "none",
        "transportSettings": {
          "path": "/trojan"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom"
    }
  ]
}
